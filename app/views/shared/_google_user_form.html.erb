<%= form_for(@google_user) do |f| %>
    <%= render 'shared/error_messages' %>

    <%= f.label :email, "Google user email" %>
    <%= f.text_field :email, class: "input-medium search-query" %>

    <%= f.label :password %>
    <%= f.password_field :password, class: "input-medium search-query" %>

    <%= f.label :password_confirmation, "Confirm Password" %>
    <%= f.password_field :password_confirmation, class: "input-medium search-query" %>

    <%= f.submit "Save changes", class: "btn btn-large btn-primary" %>
<% end %>